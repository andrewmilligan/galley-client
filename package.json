{
  "name": "galley-editor",
  "version": "0.1.0",
  "main": "./src/js/main-app.js",
  "description": "Code for the Galley Editor interactive",
  "repository": "git@github.com:andrewmilligan/galley-editor",
  "license": "ISC",
  "sideEffects": false,
  "scripts": {
    "start": "npm-run-all --parallel servedata serveapp",
    "serveapp": "webpack serve --config webpack-dev.config.js --env PROXY=8016",
    "servedata": "http-server ./src/ -a localhost -p 8016",
    "build": "webpack --mode=production --config webpack-prod.config.js",
    "test": "jest --coverage",
    "lint": "eslint \"src/**/*.js\""
  },
  "devDependencies": {
    "@associatedpress/eslint-config-interact": "^1.0.3",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-loader": "^4.0.2",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prop-types": "^15.7.2",
    "resolve-url-loader": "^3.1.3",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "to-string-loader": "^1.1.6",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "yargs": "^17.0.1"
  },
  "dependencies": {
    "core-js": "^3.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ],
      "@babel/react"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
